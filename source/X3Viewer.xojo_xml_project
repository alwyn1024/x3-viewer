  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>model As X3Core.X3Model, x As Integer, y As Integer</ItemParams>
  <ItemResult>X3Core.X3Polygon</ItemResult>
 </Method>
